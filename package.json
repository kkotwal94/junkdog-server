{
  "name": "junkdog",
  "version": "1.0.0",
  "description": "A server made by junkdog",
  "main": "server.js",
  "scripts": {
    "babel-node": "babel-node",
    "lint:js": "eslint . --fix",
    "format": "prettier --write '**/*.{js,css,md}'",
    "prepublish": "npm run-script build",
    "start": "npm run-script build && cross-env NODE_ENV=production node compiled/server.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build:dev": "cross-env NODE_ENV=development npm run webpack -- --env.server",
    "build":
      "npm run clean && cross-env NODE_ENV=production npm run webpack -- --env.browser && cross-env NODE_ENV=production npm run webpack -- --env.server",
    "webpack": "webpack --colors --display-error-details --config ./webpack/webpack.config.js",
    "flow": "flow",
    "docs": "md-magic --path '**/*.md' --ignore 'node_modules'",
    "docs:build": "documentation build 'server/**' -f html -o docs",
    "clean": "rimraf public && rimraf compiled"
  },
  "author": "Karan Kotwal",
  "license": "ISC",
  "dependencies": {
    "apollo-server-express": "^1.3.6",
    "asynx": "0.0.3",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.2",
    "connect-mongo": "^2.0.1",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.1.6",
    "cross-fetch": "^2.2.0",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "express-flash": "0.0.2",
    "express-session": "^1.15.6",
    "fs": "0.0.1-security",
    "graphql": "^0.13.2",
    "graphql-iso-date": "^3.5.0",
    "graphql-tools": "^3.0.2",
    "helmet": "^3.12.1",
    "invariant": "^2.2.4",
    "lodash": "^4.17.10",
    "merge-graphql-schemas": "^1.5.1",
    "method-override": "^2.3.10",
    "moment": "^2.22.1",
    "mongoose": "^5.1.2",
    "numeral": "^2.0.6",
    "passport": "^0.4.0",
    "path": "^0.12.7",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-apollo": "^2.1.4",
    "react-dom": "^16.4.0",
    "react-helmet": "^5.2.0",
    "react-jss": "^8.4.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.3.1",
    "serialize-javascript": "^1.5.0",
    "start-server-webpack-plugin": "^2.2.5",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "css-loader": "^0.28.11",
    "danger": "^3.7.11",
    "documentation": "^6.3.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-prettier": "^2.6.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "lint-staged": "^7.1.2",
    "markdown-magic": "^0.1.21",
    "morgan": "^1.9.0",
    "node-fetch": "^2.1.2",
    "nodemon": "^1.17.4",
    "postcss-cssnext": "^3.1.0",
    "postcss-global-import": "^1.0.6",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "postcss-reporter": "^5.0.0",
    "prettier": "^1.12.1",
    "react-transform-hmr": "^1.0.4",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.1",
    "stylelint-config-standard": "^18.2.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-manifest-plugin": "^2.0.2"
  }
}
